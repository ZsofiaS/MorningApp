{
    "env": {
        "browser": true,
        "es2020": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "React": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": "error"
    }
    "esversion": 6
}
